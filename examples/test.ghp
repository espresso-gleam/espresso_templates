import gleam/list

pub type Params { 
  Params(items: List(String))
}

pub fn render(params: params) {
  >->
  <html>
    <head>
      <title>"Espresso"</title>
    </head>
    <body class="w-full h-full">
      <h1 class="text-4xl">"This is a header"</h1>

      list.map(params.items, fn(item) {
        <p>"Thing: item"</p>
      })
    </body>
  </html>
  <-<
}
